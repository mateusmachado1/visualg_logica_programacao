Algoritmo "troco"

Var

 preco, quant, din, troco : real

Inicio

   Escreva("Preco unitario do produto: ")
   leia(preco)
   Escreva("Quantidade comprada: ")
   leia(quant)
   Escreva("Dinheiro recebido: ")
   leia(din)
   troco<- din - (preco * quant)
   Escreva("TROCO: ",troco)
   

Fimalgoritmo