Algoritmo "crescente"

Var

 x, y : inteiro

Inicio

   escreva("Digite dois numeros: ")
   leia(x)
   leia(y)
   
    enquanto x <> y faca
      se x < y entao
        escreval("Crescente")
      senao
        escreval("Decrescente")
      fimse
      
    escreva("Digite outros dois numero: ")
     leia(x)
     leia(y)
     
     fimenquanto



Fimalgoritmo